{
  "profiles": {
    "http": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "httpPort": 8082,
      "applicationUrl": "http://localhost:8082"
    },
    "https": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true
    },
    "Docker": {
      "commandName": "Docker",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://*:443;http://*:8080"
      },
      "DockerfileRunArguments": "--memory-reservation 500m -m 650m --memory-swap 651m",
      "httpPort": 8080,
      "launchUrl": "{Scheme}://{ServiceHost}",
      "sslPort": 443,
      "useSSL": true
    }
  }
}