{
  "profiles": {
    "http": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "httpPort": 8082,      
      "applicationUrl": "http://localhost:8082"
    },
    "https": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      //"applicationUrl": "http://localhost:443;https://localhost:8010"
    },
    "Docker": {
      "commandName": "Docker",
      "environmentVariables": {        
        "ASPNETCORE_URLS": "https://localhost:8082;http://localhost:8083"
      },
      "DockerfileRunArguments": "--memory-reservation 500m -m 650m --memory-swap 651m",
      "httpPort": 8082,
      "launchUrl": "{Scheme}://{ServiceHost}",
      "sslPort": 8083,
      "useSSL": true
    }
  }
}