{
  "profiles": {
    "http": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "httpPort": 8082,
      "applicationUrl": "http://localhost:8082"
    },
    "https": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true
    },
    "Docker": {
      "commandName": "Docker",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://*:8084;http://*:8083"
      },
      "DockerfileRunArguments": "--memory-reservation 500m -m 650m --memory-swap 651m",
      "httpPort": 8083,
      "launchUrl": "{Scheme}://{ServiceHost}",
      "sslPort": 8084,
      "useSSL": true
    }
  }
}